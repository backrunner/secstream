{
  "name": "secstream",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.15.0",
  "description": "Secure audio streaming library with client/server components",
  "author": "BackRunner",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vitest",
    "build": "rimraf ./dist && rollup -c",
    "test": "vitest run",
    "lint": "eslint ./src/**/*",
    "lint:fix": "eslint ./src/**/* --fix"
  },
  "dependencies": {
    "fflate": "^0.8.2",
    "nanoid": "^5.0.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.1.0",
    "@types/web": "^0.0.166",
    "eslint": "^9.31.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.0",
    "rollup-plugin-dts": "^6.2.3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vitest": "^2.0.0"
  }
}
